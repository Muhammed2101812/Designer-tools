{
  "name": "design-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 127.0.0.1 -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "check-env": "tsx scripts/check-env.ts",
    "verify-env": "tsx scripts/verify-environment-setup.ts",
    "verify-deployment": "tsx scripts/verify-deployment.ts",
    "verify-production": "tsx scripts/verify-production.ts",
    "setup-monitoring": "tsx scripts/setup-monitoring.ts",
    "backup-db": "tsx scripts/backup-database.ts",
    "monitoring-dashboard": "tsx scripts/monitoring-dashboard.ts",
    "monitoring-watch": "tsx scripts/monitoring-dashboard.ts --watch",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > lib/supabase/types.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:performance": "vitest --run lib/utils/__tests__/performance.test.ts",
    "test:e2e": "playwright test",
    "perf:audit": "tsx scripts/performance-audit.ts",
    "perf:processing": "tsx scripts/measure-processing-time.ts",
    "perf:memory": "tsx scripts/memory-profiler.ts",
    "perf:all": "npm run test:performance && npm run perf:processing && npm run perf:memory",
    "dev:turbo": "next dev --turbo",
    "perf:check": "node scripts/dev-performance-check.js",
    "cache:clear": "rmdir /s /q .next 2>nul || echo \"Cache cleared\"",
    "test:lighthouse": "node scripts/lighthouse-audit.js",
    "test:lighthouse-ci": "tsx scripts/lighthouse-ci.ts",
    "test:performance-budget": "tsx scripts/lighthouse-ci.ts --fcp 1500 --lcp 2500 --cls 0.1",
    "test:security-headers": "node scripts/security-headers-test.js",
    "test:rate-limit": "node scripts/rate-limit-test.js",
    "test:security-vulns": "node scripts/security-vulnerability-test.js",
    "test:performance-security": "node scripts/performance-security-test-suite.js",
    "prepare-production": "tsx scripts/prepare-production-deployment.ts",
    "update-stripe-webhook": "tsx scripts/update-stripe-webhook.ts",
    "run-production-migrations": "tsx scripts/run-production-migrations.ts",
    "deployment-checklist": "tsx scripts/production-deployment-checklist.ts",
    "deploy:setup": "tsx scripts/production-deployment-setup.ts",
    "deploy:summary": "tsx scripts/production-deployment-summary.ts",
    "validate:env": "tsx scripts/validate-production-env.ts",
    "validate:production": "npm run validate:env && npm run verify-production",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "analyze:bundle": "tsx scripts/bundle-analysis.ts",
    "analyze:simple": "tsx scripts/simple-bundle-analysis.ts",
    "audit:deps": "tsx scripts/dependency-audit.ts",
    "optimize:bundle": "npm run audit:deps && npm run analyze:simple",
    "validate:performance": "tsx scripts/performance-validation.ts",
    "validate:deployment": "tsx scripts/deployment-validation.ts",
    "measure:improvements": "tsx scripts/measure-performance-improvements.ts",
    "validate:all": "npm run validate:performance && npm run validate:deployment",
    "performance:baseline": "tsx scripts/performance-validation.ts baseline",
    "performance:complete": "npm run measure:improvements"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^10.20.0",
    "@stripe/stripe-js": "^8.1.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.43.0",
    "@types/qrcode": "^1.5.5",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.6",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.378.0",
    "next": "^14.2.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^3.3.0",
    "resend": "^6.2.2",
    "stripe": "^19.1.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.0.4",
    "@lhci/cli": "^0.12.0",
    "autoprefixer": "^10.4.19",
    "chrome-launcher": "^1.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.0",
    "glob": "^11.0.3",
    "jsdom": "^27.0.1",
    "lighthouse": "^12.8.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
